<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="regression suite" verbose="1">
    <groups>
        <run>
            <include name="regression">
            </include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.tutorialsninja.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="regression Test">
        <classes>
            <class name="com.tutorialsninja.testsuite.HomePageTest"></class>
            <class name="com.tutorialsninja.testsuite.DesktopTest"></class>
            <class name="com.tutorialsninja.testsuite.LaptopsAndNotebooksTest"></class>
            <class name="com.tutorialsninja.testsuite.MyAccountsTest"></class>

        </classes>
    </test>
</suite>